{
  "name": "neumorphic-dashboard",
  "version": "2.0.0",
  "description": "Dashboard interattiva con design Neumorphic moderno - Include orologio analogico, player audio, e componenti UI interattivi",
  "main": "index.js",
  "scripts": {
    "start": "python3 -m http.server 8080",
    "sass:watch": "sass --watch style.scss:style.css",
    "sass:build": "sass style.scss:style.css --style=compressed",
    "serve": "python3 -m http.server 8080"
  },
  "keywords": [
    "dashboard",
    "neumorphic",
    "design",
    "ui",
    "components",
    "audio-player",
    "interactive"
  ],
  "author": "Fabio Murtas",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/profabiuls/dash.git"
  },
  "devDependencies": {
    "sass": "^1.69.5"
  }
}
